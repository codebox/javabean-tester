                            <artifactId>org.eclipse.jgit</artifactId>
                            <version>3.7.1.201504261725-r</version>
                        </dependency>
                        <dependency>
                            <groupId>org.codehaus.plexus</groupId>
                            <artifactId>plexus-utils</artifactId>
                            <version>3.0.22</version>
